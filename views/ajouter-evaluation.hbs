<script type="text/javascript">
        $(document).ready(function () {
            $('#ajout_submit').click(function () {
                var payload = {
                    CODE_FORMATION: $('#formation').val() ,
					ANNEE_PRO: $('#promotion').val() ,
					CODE_UE: $('#ue').val() ,  
					DESIGNATION: $('#designation').val() ,
					//DEBUT_REPONSE: $('#debut').val() , 
					//FIN_REPONSE: $('#fin').val()
                };

                $.ajax({
                    url: "/evalajoute",
                    type: "POST",
                    contentType: "application/json",
                    processData: false,
                    data: JSON.stringify(payload),
                    complete: function (data) {
                        $( ".formClass" ).append("<div class='alert alert-success'><strong>Evaluation Ajoute avec succes</strong></div>");

                    }
                });
            });
        });
    </script>
	<script type="text/javascript">
        $(document).ready(function () {
            $('#promotion').change(function () {
                var payload = {
                    ANNEE_PRO: $('#promotion').val() 
				};

                $.ajax({
                    url: "/ajouterEval",
                    type: "GET",
                    contentType: "application/json",
                    processData: false,
                    data: JSON.stringify(payload),
                    complete: function (data) {
                    $( ".formClass" ).append("<div class='alert alert-success'><strong>Evaluation Ajoute avec succes</strong></div>");

                    }
                });
            });
        });
    </script>

